<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gerdi.doadoa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chargingimans"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.gerdi.doadoa.ItemDetails"></activity>
        <activity android:name="com.gerdi.doadoa.View.ListDoa" />
        <activity android:name="com.gerdi.doadoa.View.recycler" />
        <activity android:name="com.gerdi.doadoa.View.MainActivity" />
        <activity android:name="com.gerdi.doadoa.View.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>